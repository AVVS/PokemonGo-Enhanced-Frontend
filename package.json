{
  "name": "pokemongo-react",
  "version": "0.0.0",
  "private": true,
  "main": "bin/server.js",
  "engines": {
    "node": ">=6.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "npm run lint && better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "deploy:docker": "better-npm-run deploy:docker",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "docker": "./scripts/release.sh"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "API_URL": "http://localhost:3000"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:docker": {
      "command": "npm run docker",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PokemonGo-Enhanced-Frontend.git"
  },
  "dependencies": {
    "better-npm-run": "0.0.10",
    "debug": "^2.2.0",
    "fs-extra": "^0.30.0",
    "koa": "^2.0.0-alpha.3",
    "koa-compress": "^2.0.0",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.6.0",
    "koa-static": "^2.0.0",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "axios": "^0.13.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.11.5",
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.4.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.5.0",
    "chance": "^1.0.3",
    "cheerio": "^0.20.0",
    "classnames": "^2.2.5",
    "codecov": "^1.0.1",
    "color": "^0.11.3",
    "enzyme": "^2.4.1",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "history": "^2.1.2",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "json": "^9.0.4",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.8.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.3",
    "pokemongo-data": "^1.1.2",
    "radium": "^0.18.1",
    "radium-grid": "^2.0.1",
    "radium-normalize": "^2.0.3",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-color": "^2.2.0",
    "react-dom": "^15.2.1",
    "react-lazyload": "^2.1.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-router-scroll": "^0.2.0",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.20.2",
    "redbox-react": "^1.2.10",
    "redux": "^3.0.0",
    "redux-actions": "^0.10.0",
    "redux-form": "^6.0.0-rc.3",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk-fsa": "^3.0.0",
    "reselect": "^2.5.3",
    "rimraf": "^2.5.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  }
}
